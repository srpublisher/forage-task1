          $("#ishispecialblock-1526476091 .item").each(function() {
            var container = $(this).find('.countdown-container');
            var time = $(this).data('countdowntime');
            $(container).countdown(time, function(event) {
              $(this).find(".countdown-days .data").html(event.strftime('%D'));
              $(this).find(".countdown-hours .data").html(event.strftime('%H'));
              $(this).find(".countdown-minutes .data").html(event.strftime('%M'));
              $(this).find(".countdown-seconds .data").html(event.strftime('%S'));
              });
          });

          $('#ishispecialblock-1526476091 .owl-carousel').owlCarousel({
              loop:false,
              nav:false,
              rewind:true,
              dots:true,
              margin:30,
              rtl: $('html').attr('dir') == 'rtl'? true : false,
              navText: ["<i class='fa fa-angle-left'></i>","<i class='fa fa-angle-right'></i>"],
              responsive:{
                  0:{
                      items:2,
                      margin:0
                  },
                  544:{
                      items:2
                  },
                  768:{
                      items:2
                  },
                  992:{
                      items:3
                  },
                  1200:{
                      items:4
                  }
              }
          });

          // $(document).on('click','#ishispecialblock-1526476091 .deal-product-thumbnails',function () {
          //   $(".thumb-container").removeClass('active');
          //   $(this).parents('.thumb-container').find('img').attr('src',$(this).attr('src'));
          //   $(this).parents('.thumb-container').addClass('active');
          // });

          $(document).on('click','#ishispecialblock-1526476091 .product-img-extra',function () {
            $(".thumb-container").removeClass('active');
            $(this).parents('.product-thumb').find('.image img').attr('src',$(this).attr('src'));
            $(this).parents('.thumb-container').addClass('active');
          });

